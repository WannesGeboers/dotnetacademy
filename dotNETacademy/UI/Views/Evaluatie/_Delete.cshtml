@model dotNETacademy.Models.Evaluatie

<h3>Wil je echt de evaluatie van @Html.DisplayFor(model => model.Deelnemer.Naam) wissen?</h3>
<div>
    <hr />

    <form asp-action="Delete">
        <div class="text-center">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Ja" class="btn btn-danger" />
            <a class="btn btn-success" asp-action="Index">Nee</a>
        </div>
    </form>
</div>